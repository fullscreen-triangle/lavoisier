{
  "ms_parameters": {
    "ms1_threshold": 1000,
    "ms2_threshold": 100,
    "mz_tolerance": 0.01,
    "rt_tolerance": 0.5,
    "min_intensity": 500,
    "output_dir": "output",
    "n_workers": -1
  },
  "compute_parameters": {
    "chunk_size_mb": 1024,
    "memory_fraction": 0.8,
    "use_gpu": false
  },
  "storage": {
    "format": "zarr",
    "compression": "lz4",
    "chunk_size": 1000000
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(levelname)s - %(message)s",
    "file": "ms_processing.log"
  }
} 